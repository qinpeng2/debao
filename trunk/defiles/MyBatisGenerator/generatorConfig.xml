        <table tableName="driftlog" domainObjectName="DriftLogVO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"          enableSelectByExample="false" selectByExampleQueryId="false">
        </table>
        <table tableName="internal" domainObjectName="InternalVO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"          enableSelectByExample="false" selectByExampleQueryId="false">
        </table>
        <table tableName="internallog" domainObjectName="InternalLogVO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"          enableSelectByExample="false" selectByExampleQueryId="false">
        </table>
        <table tableName="external" domainObjectName="ExternalVO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"          enableSelectByExample="false" selectByExampleQueryId="false">
